/* pages/kepu/kepu.wxss */
page{
  background-image: url('data:image/png;base64,');
}
.master .top{
  margin-right: 40rpx;
}
.master .top .lv-back{
  background-color:#BFF0BF;
  width: 100%;
  height: 300rpx;
  display: flex;
  border-radius: 30rpx;
  margin: 20rpx;
}
.master .top .lv-back image{
  flex:1;
  height: 250rpx;
  height: 250rpx;
  margin:5rpx 20rpx;
}
.master .top .lv-back .zhishiku{
  flex:1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 65rpx;
  font-weight: 400;
  padding-right:3%;
}


.master .top .mode{
  display: flex;
  margin:60rpx 20rpx;
}
.master .top .mode .card_list{
  background-color: white;
  border-radius: 10%;
  margin:0 20rpx;
  flex:1;
  justify-content: center;
  align-items: center;
  display: flex;
  flex-direction: column;
}
.master .top .mode .card_list .card_icon{
  height: 100rpx;
  width: 100rpx;
  margin:10rpx 0;
}
.master .top .mode .title{
  margin: 10rpx;
}

.master .bottom{
  margin-right: 40rpx;
}
.master .bottom .guanyu-back{
  background-color:#CCE2FF;
  width: 100%;
  height: 300rpx;
  display: flex;
  border-radius: 30rpx;
  margin: 20rpx;
}
.master .bottom .guanyu-back image{
  flex:1;
  height: 250rpx;
  height: 250rpx;
  margin:5rpx 20rpx;
}
.master .bottom .guanyu-back .guanyuld{
  flex:1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 65rpx;
  font-weight: 400;
  padding-right:3%;
}

.master .bottom .bottom_mode{
  display: flex;
  margin:60rpx 20rpx;
  padding: 0 80rpx;
}
.master .bottom .bottom_mode .card_list{
  background-color: white;
  border-radius: 10%;
  margin:0 40rpx;
  flex:1;
  justify-content: center;
  align-items: center;
  display: flex;
  flex-direction: column;
}
.master .bottom .bottom_mode .card_icon{
  height: 140rpx;
  width: 140rpx;
  margin:10rpx 0;
}
.master .bottom .bottom_mode .title{
  margin-bottom: 10rpx;
}
.master .bottom_caidan{
  display: flex;
  align-items: center;
  justify-content: center;
}
.master .bottom_caidan image{
  margin-top: -10rpx;
  height: 50rpx;
  width: 50rpx;
}
.commodity_screen{
  width: 100%;
  height: 100%;
  position: fixed;
  top:0;
  background-color: rgb(42, 40, 40);
  opacity: 0.5;
  z-index: 1000;
}
.commodity_attr_box{
  height: 550rpx;
  border-radius: 20rpx;
  width: 80%;
  overflow: hidden;
  position: fixed;
  bottom: 30%;
  margin-left: 10%;
  margin-right: 10%;
  z-index: 2000;
  background: #fff;
  padding-top:20rpx;
  display: flex;
  flex-direction: column;
}
.commodity_attr_box .cont_shan{
  flex:1;
}
.commodity_attr_box .cont_shan image{
  height: 50rpx;
  width: 50rpx;
  float: right;
}
.commodity_attr_box scroll-view{
  flex:4;
}
.commodity_attr_box .caidan_list{
  display: flex;
  flex-wrap: wrap;
  margin: -10rpx 20rpx;
}
.commodity_attr_box  .caidan_list navigator{
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  width: 33.33%;
  text-align: center;
  margin: 15rpx 0rpx;
}
.commodity_attr_box  .caidan_list navigator .lvdao_icon{
  flex:2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.commodity_attr_box .caidan_list navigator .lvdao_icon image{
  border-radius: 40%;
  width: 50%;
  overflow:hidden;
}
.commodity_attr_box .caidan_list navigator .lvdao_name{
  flex:1;
  justify-content: center;
  font-size: 30rpx;
  align-items: center;
}